{
  "name": "linux-orchestrator",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Linux desktop automation orchestrator with recursive testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:orchestrator": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:server": "jest src/server/",
    "test:extension": "jest src/test/suite/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "express": "^5.1.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
